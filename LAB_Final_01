#include <iostream>
#include <iomanip>
using namespace std;
int main() {
	cout << "Greylen Fernandez Final Exam Lab Practical (1 of 4)" << endl;
	//Creating the loop
	for (int x = 1; x <= 100; x++) {
		cout << setw(3) << setfill('0') << x;
		if (x % 10 != 0)
			cout << ", ";
		else
			cout << endl;
	}

}
